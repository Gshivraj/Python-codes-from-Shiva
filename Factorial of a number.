#Normal programme
n = int(input("enter the value of n")
fact =1 
i =0

while i <=n:
    fact = n*(n-1)
    i = i+1 
print(fact)
     
 #Using function
def factorial(x):
    if x ==0  or x ==1:
        return 1
     else:
        print(x*factorial(x-1))

x = int(input("enter the value of x")
factorial(x)
