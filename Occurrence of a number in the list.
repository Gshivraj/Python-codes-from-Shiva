#Lets say that we have to count the number of tyimes number 3 occurs in the list [1,5,8,9,3,3,2,8]

Regular programme

lsts = []
count = 0
n = int(input("enter the value of n")
for i in range(n):
    e = int(input("enter the values of list")
    lsts.append(e)
for i in lsts:
    if i ==3
    count = count+1
print("the number of times the number 3 occured is " count)





# Using function

def occurrence(lsts):
    count = 0
n = int(input("enter the value of n")
for i in range(n):
    e = int(input("enter the values of list")
    lsts.append(e)
for i in lsts:
    if i ==3:
        count = count+1
    print(count)

occurrence(lsts)
    
