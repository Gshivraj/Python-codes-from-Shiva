# Regular programme
S = []
p = max(S)
v = []
n = int(input("enter the numbers")
for i in range(n):
    g = int(input("enter the values")
    S.append(g)
for i in S:
    e = p-i
    v.append(e)

print("the second largest number is", max(v))




#Using function
def maxmin(s):
   n = int(input("enter the numbers")
for i in range(n):
    g = int(input("enter the values")
    s.append(g)
    for i in s:
        e = p-i
        v.append(e)
        print(max(v))
maxmin(s)
