# Regular programme.
n=int(input("enter the value of n")
Count = 0
for i in range(2,n+1):
    if n%i==0:
    count = count+1
if count == 1
  print("The number is prime")
else:
print("the number is not prime")



#Using functions
 def prime(n):
 count = 0
 for in range (2,n+1)
    if n%i==0:
    count = count+1
 if count ==1:
   return "prime number"
else:
   return "not a prime number"
result = prime(91)
print(result)
    
